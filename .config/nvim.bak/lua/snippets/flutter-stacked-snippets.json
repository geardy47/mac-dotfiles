{
  "Stacked View": {
    "prefix": "stv",
    "body": [
      "import 'package:flutter/material.dart';",
      "import 'package:stacked/stacked.dart';",
      "",
      "import '${2:viewModel}';",
      "",
      "class ${1:view} extends StackedView<${1:view}Model> {",
      "  const ${1:view}({Key? key}) : super(key: key);",
      "",
      "  @override",
      "  ${1:view}Model viewModelBuilder(BuildContext context) => ${1:view}Model();",
      "",
      "  @override",
      "  Widget builder(",
      "    BuildContext context,",
      "    ${1:view}Model viewModel,",
      "    Widget? child,",
      "  ) {",
      "    return const Scaffold(",
      "      body: SafeArea(",
      "        child: Center(",
      "          child: Text('${1:view}'),",
      "        ),",
      "      ),",
      "    );",
      "  }",
      "}",
      ""
    ],
    "description": "Stacked View Snippet"
  },
  "Stacked View Model": {
    "prefix": "stvm",
    "body": [
      "import 'package:stacked/stacked.dart';",
      "",
      "class ${1:viewModel} extends BaseViewModel {}",
      ""
    ],
    "description": "Stacked ViewModel Snippet"
  }
}
